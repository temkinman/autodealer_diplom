@model IEnumerable<string>

<div class="d-grid gap-2">
	@foreach (string category in Model)
	{
		<a class="btn @(category == ViewBag.SelectedCategory ? "btn-dark" : "btn-outline-dark")" 
		   asp-action="Index"
		   asp-controller="Car"
		   asp-route-category="@category"
		   asp-route-currentPage="1">
			@category
		</a>
	}
</div>