@model AutoDealer.Web.ViewModel.CarModelViewModel

<h1>Админ панель для моделей авто</h1>
@{
   await Html.PartialAsync("Notification");
}

<div class="row">
    <div class=" col-6 d-flex align-items-center justify-content-between">
        <label>Компания</label>
        <select asp-for="@Model.CompanyId" class="form-control" asp-items="@(new SelectList(@Model.Companies,"Id","Title"))">
            <option value="0">Выберите</option>
        </select>
        <button id="filterBtn" class="btn btn-outline-secondary">Применить</button>
    </div>
</div>
<div id="modelContent"></div>

<script type="text/javascript" src="./js/carModel.js"></script>